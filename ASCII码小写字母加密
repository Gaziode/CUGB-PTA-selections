#include<iostream>
using namespace std;

int main()
{
	char a = 0, b = 0, c = 0;
	cin >> a;
	b = a + 3 - 32;
	c = a - 23 - 32;
	if (a > 96 && a < 120)
		cout << b << "," << int(b);
	else if (a > 119 && a < 123)
		cout << c << "," << int(c);
	else
		cout << a << "," << int(a);
	
	return 0;

}//小心最后三位
