#include<iostream>
using namespace std;
const int N=10;
void BubbleSort(int a[],int n); 
int main()
{
	int a[N],i;
	for(i=0;i<N;i++)
		cin>>a[i];
	
	BubbleSort(a,N);

	for(i=0;i<N;i++)
		cout<<a[i]<<" ";
	return 0;
}
void BubbleSort(int a[],int n)
{
	int i,j;
	for(j=0;j<n;j++)//运行次数为数组中的元素个数
		for(i=0;i<n-j;i++)//每次比较n-j次
			if(a[i]>a[i+1])
			{
				int t=a[i];//将较大的数移到数组的末尾
				a[i]=a[i+1];
				a[i+1]=t;
			}	
}
